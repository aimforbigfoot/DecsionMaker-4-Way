config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="DecisionMaker 4Way"
run/main_scene="res://src/HomeScreen.tscn"
run/low_processor_mode=true
config/icon="res://assets/icons/icon.png"
config/macos_native_icon="res://assets/icons/icon.png"
